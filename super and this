class Person{
	String name,address,emailid;
	int phoneno;
//	Person(){
//		System.out.println("default constructor");
//	}
	Person(String name, String address, String emailid){
		this.name = name;
		this.address = address;
		this.emailid = emailid;
	}
	final void display(){
		System.out.println("Person  name ="+name);
		System.out.println("person addres ="+address);
		System.out.println("Person emailid ="+emailid);
	}
}
[1:43 pm, 02/06/2021] Dr. Prashanth Sir RVCE: package inheritance;

class B{
	int b,c;
	String name,address, emailid;
	B(int a){   //not a default constructor
		System.out.println("constructor in B");
	}
	void fun() {
		System.out.println("inside the method b");
	}	
}

public class A extends B{
	A(int a){
		super(10); //B b = new B(10);  // should be used inside the constructor
		super.name = "bbmp";
		super.fun();
		System.out.println("constructor in A");
	}
	void fun1() {
		super.name = "rvce";
	}
	public static void main(String args[]) {
		A a = new A(10);
		a.fun();
	}
 
}
