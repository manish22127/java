public class Employee extends Person{
		final int a=10;
        String company,empid;
        Employee(String name, String address, String emailid,String company, String empid){
                super(name,address,emailid);
				this.name=name;
				this.address = address;
				this.emailid = emailid;
//				super.display();
				this.company = company;
                this.empid = empid;
        }
        @Override
       void display(){
               System.out.println("Employee name ="+super.name);
               System.out.println("Employee addres ="+super.address);
               System.out.println("Employee emailid ="+super.emailid);
               super.display();
               System.out.println("Employee company = "+company);
               System.out.println("Employee empid = "+empid);
			
        }
	public static void main(String args[]){
		Employee e = new Employee("xyz","kengeri","xyz@rvce.edu.in","rvce","rv123");
		//e.display();
		
////		Employee e1 = new Employee("abc","bannerghatta","abc@rvce.edu.in","rvce","rvce1234");
//		Person p = (Person)e;
//		p.display();
//	//	System.out.println(Examp_Array.hello());
//	//	e.display();
//	//	e1.display();
	}
}
