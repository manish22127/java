class Person{

	String name,address,emailid;
	int phoneno;
	Person(){
		System.out.println("default constructor");
	}
	Person(String name, String address, String emailid){
		this.name = name;
		this.address = address;
		this.emailid = emailid;
	}
	void display(){
		System.out.println("Person Employes name ="+name);
		System.out.println("Employee addres ="+address);
		System.out.println("Person Employee emailid ="+emailid);
	}
}
[11:08 am, 01/06/2021] Dr. Prashanth Sir RVCE: package inheritance;

public class Employee extends Person{

        String company,empid;

        Employee(String name, String address, String emailid,String company, String empid){
                super(name,address,emailid);
		this.name=name;
		this.address = address;
		this.emailid = emailid;
		super.display();
		this.company = company;
                this.empid = empid;
        }
